  "directories": {
    "example": "examples",
    "test": "test"
  },
  "keywords": [
    "javascript",
    "framework",
    "browserify",
    "performance",
    "crossplatform"
  ],
  "license": "GPL",
  "devDependencies": {
    "dmd": "^1.1.1",
    "jsdoc-to-markdown": "^1.1.1",
    "gaston": "git+ssh://git@github.com:vigour-io/gaston.git#develop"
  },
  "gaston": {
    "source-maps": false
  },
  "browser": {
    "./lib/base/iterator/": "./lib/base/iterator/browser.js"
  },
  "vigour": {
    "services": {
      "sentinel": {
        "branches": [],
        "unignoreFolders": []
      }

{
  "name": "vjs",
  "version": "0.2.1",
  "author": "Vigour.io <dev@vigour.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/vjs.git"
  },
  "engines": {
    "node": "^4.1.1"
  },
  "scripts": {
    "test": "./node_modules/.bin/gaston test browser -f test/common/",
